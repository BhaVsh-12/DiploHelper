<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_account"
        android:name="com.bhavesh.diplohelper.ui.account.AccountFragment"
        android:label="Account"
        tools:layout="@layout/fragment_account"/>
    <fragment
        android:id="@+id/nav_contact"
        android:name="com.bhavesh.diplohelper.ui.contact.ContactFragment"
        android:label="ChatAi"
        tools:layout="@layout/fragment_contact" />
    <fragment
        android:id="@+id/nav_home"
        android:name="com.bhavesh.diplohelper.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.bhavesh.diplohelper.ui.gallery.GalleryFragment"
        android:label="Chat Room"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.bhavesh.diplohelper.ui.slideshow.SlideshowFragment"
        android:label="Contact"
        tools:layout="@layout/fragment_slideshow" />

